{
  "name": "guess-mine",
  "version": "1.0.0",
  "engines": {
    "yarn": "1.16.0",
    "node": "10.16.0"
  },
  "description": "Realtime Drawing game built with SocketIO, Gulp, and Node",
  "main": "index.js",
  "repository": "git@github.com:taeyoungs/guess-mine.git",
  "author": "taeyoung <xoxodudwkd@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "babelify": "^10.0.0",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-browserify": "^0.5.1",
    "gulp-csso": "^3.0.1",
    "gulp-sass": "^4.0.2",
    "lodash": "^4.17.15",
    "lodash.template": "^4.5.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "pug": "^2.0.4",
    "shell-quote": "^1.7.2",
    "socket.io": "^2.4.0",
    "uglify-js": "^3.6.7"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/server --ignore assets/ --ignore src/static",
    "dev:assets": "gulp",
    "build:assets": "gulp",
    "build:server": "babel src --out-dir build --ignore 'src/static'",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "yarn run build:server && yarn run copyAll",
    "prebuild": "rm -rf build",
    "start": "node build/server",
    "prestart": "yarn run build"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1"
  }
}
